{
    "compileOnSave": true,
    "compilerOptions": {
        "lib": ["es2015"],
        "module": "commonjs",
        "target": "es2017",
        "moduleResolution": "node",
        "inlineSourceMap": true,
        "sourceMap": false,
        "declaration": false,
        "removeComments": false,
        "outDir": "binDir",
        "esModuleInterop": true,
        "resolveJsonModule": true,
        "types": ["node", "protractor-react-selector"]
    },
    "exclude": ["node_modules"]
}